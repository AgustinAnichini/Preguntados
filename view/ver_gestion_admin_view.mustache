<main>
    <div class="bg-warning p-3">
        <h1 class="text-center p-3 text-dark">SABIONDOS</h1>
    </div>


    {{#espacioGestionUsuarios}}
        <div class="container d-flex justify-content-between align-items-center mt-5">
            <form action="/Admin/adminUsuarios" method="post" class="botones">
                <input type="submit" name="volverLobby" id="volverLobby" class="btn btn-outline-warning" value="Lobby">
            </form>
        </div>    {{/espacioGestionUsuarios}}
    {{#espacioGestionPreguntas}}
        <div class="container d-flex justify-content-between align-items-center mt-5">
            <form action="/Admin/adminPreguntas" method="post" class="botones">
                <input type="submit" name="volverLobby" id="volverLobby" class="btn btn-outline-warning" value="Lobby">
            </form>
        </div>
    {{/espacioGestionPreguntas}}
    {{#espacioGestionPartidas}}
        <div class="container d-flex justify-content-between align-items-center mt-5">
            <form action="/Admin" method="post" class="botones">
                <input type="submit" name="volverLobby" id="volverLobby" class="btn btn-outline-warning" value="Lobby">
            </form>
        </div>
    {{/espacioGestionPartidas}}
    <h1 class="text-center p-3 text-light">{{#tituloGestion}}{{tituloGestion}}{{/tituloGestion}}</h1>



    {{#cantidad}}
        <h3 class="text-light text-center">Cantidad: {{cantidad}}</h3>
    {{/cantidad}}

    {{#grafico_base64}}
        <div class="container text-center">
            <img src="data:image/png;base64,{{grafico_base64}}" alt="Gráfico de Usuarios por país" class="img-fluid">
        </div>
    {{/grafico_base64}}



    {{#cantidadMenores}}
        <h3 class="text-light text-center">Cantidad menores: {{cantidadMenores}}</h3>
    {{/cantidadMenores}}

    {{#cantidadAdultos}}
        <h3 class="text-light text-center">Cantidad adultos: {{cantidadAdultos}}</h3>
    {{/cantidadAdultos}}
    {{#cantidadJubilados}}
        <h3 class="text-light text-center">Cantidad jubilados: {{cantidadJubilados}}</h3>
    {{/cantidadJubilados}}


    <div class="container w-25 contendorMSJ" id="mensajeSugerir">
        {{#mensajeUsuarioSugerir}}
            <a id="boton" class="cerrar"><img src="../public/img/x-circle.svg"></a>
            <p class="text-light text-center bg-primary p-1 rounded">{{mensajeUsuarioSugerir}}</p>
        {{/mensajeUsuarioSugerir}}
    </div>

</main>
